{
    "name": "@pzh-ui/components",
    "version": "0.0.185",
    "description": "Contains default components to use in Provincie Zuid-Holland projects.",
    "license": "ISC",
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "test:generate-output": "NODE_ENV=testing jest --json --outputFile=.jest-test-results.json || true",
        "test": "NODE_ENV=testing jest",
        "prepare": "tsc && vite build",
        "storybook": "npm run prebuild:storybook && start-storybook -p 6006",
        "prebuild:storybook": "npm run test:generate-output",
        "build:storybook": "build-storybook",
        "build": "vite build",
        "chromatic": "npx chromatic --project-token=9c6f933cf59e --auto-accept-changes"
    },
    "dependencies": {
        "@floating-ui/react-dom-interactions": "^0.3.1",
        "@headlessui/react": "^1.5.0",
        "@pzh-ui/icons": "^0.0.11",
        "@types/react-table": "^7.7.10",
        "classnames": "^2.3.1",
        "date-fns": "^2.28.0",
        "formik": "^2.2.9",
        "framer-motion": "^6.3.0",
        "mime": "^3.0.0",
        "react": "^18.0.0",
        "react-datepicker": "^4.7.0",
        "react-dom": "^18.0.0",
        "react-dropzone": "^12.0.4",
        "react-quill": "^2.0.0-beta.4",
        "react-router-dom": "^6.3.0",
        "react-select": "^5.2.2",
        "react-table": "^7.7.0",
        "react-use": "^17.3.2"
    },
